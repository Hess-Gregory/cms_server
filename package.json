{
  "name": "app-express",
  "version": "1.0.0",
  "description": "server node",
  "main": "./app/server",
  "scripts": {
    "start": "nodemon ./app/server --exec babel-node --presets env | bistre",
    "release": "npm run clean && npm run build && npm run serve",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel . -s -D -d dist --presets env --ignore node_modules",
    "test": "./bin/test.sh",
    "lint": "standard"
  },
  "author": "HESS Gregory",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "bistre": "^1.0.1",
    "body-parser": "^1.19.0",
    "bole": "^4.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "formidable": "^1.2.2",
    "http-status": "^1.5.0",
    "mv": "^2.1.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.12",
    "sequelize": "^6.6.5",
    "standard": "^16.0.3",
    "supertest": "^6.1.4",
    "tap-dot": "^2.0.0",
    "tape": "^5.2.2"
  }
}
